{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Autocomplete",
			"type": "shell",
			"group": "build",
			"command": "lua",
			"args": [
				".\\builders\\emmy.lua"
			],
			"problemMatcher": []
		},
		{
			"label": "Build Autodocs",
			"type": "shell",
			"command": "lua",
			"args": [
				".\\builders\\readthedocs.lua"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": []
		},
		{
			"label": "Build All",
			"command": "echo",
			"args": [
				"Build complete!"
			],
			"type": "shell",
			"group": "build",
			"dependsOn": [
				"Build Autocomplete",
				"Build Autodocs"
			],
			"problemMatcher": []
		}
	]
}